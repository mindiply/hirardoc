{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "12"
      }
    }],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "transform-dirname-filename",
    "transform-line",
    ["babel-plugin-inline-import", {
      "extensions": [
        ".idl"
      ]
    }]
  ]
}
